<editors>

    <!-- Boolean -->
    <editor id="bool">
        <range uom="2" subset="0,1" />
    </editor>

    <!-- Log Debug Mode -->
    <editor id="DEBUGMODE">
      <range uom="25" subset="0,10,20,30,40,50" nls="CDM" />
    </editor>

    <!-- Pushover Priority -->
    <editor id="PO_PRIORITY">
      <range uom="25" subset="0-4" nls="POVP" />
    </editor>

    <!-- Pusover User -->
    <editor id="PO_ERR">
      <range uom="25" subset="0-5" nls="POVE" />
    </editor>

    <!-- Assistant Relay User -->
    <editor id="ASR_USER">
      <range uom="25" subset="0-1" nls="ASRU" />
    </editor>

    <!-- Pushover Priority -->
    <editor id="NFY_PRIORITY">
      <range uom="25" subset="0-4" nls="NFYP" />
    </editor>


</editors>
