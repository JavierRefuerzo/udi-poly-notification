              <cmd id="SET_MESSAGE_DOF">
                <p id="" editor="NMESSAGE" init="GV2"/>
              </cmd>
              <cmd id="SET_DEVICE">
                <p id="" editor="{1}" init="GV4"/>
              </cmd>
              <cmd id="SET_PRIORITY">
                <p id="" editor="PO_PRIORITY" init="GV5"/>
              </cmd>
              <cmd id="SET_FORMAT">
                <p id="" editor="PO_FORMAT" init="GV6"/>
              </cmd>
              <cmd id="DON" />
              <cmd id="DOF" />
            </accepts>
        </cmds>
    </nodeDef>
</nodeDefs>
