            <st id="ST"  editor="bool" />
            <st id="GV1" editor="NMESSAGE" />
            <st id="GV2" editor="NMESSAGE" />
            <st id="GV3" editor="NFY_NODE" />
            <st id="GV4" editor="{1}" />
            <st id="GV5" editor="PO_PRIORITY" />
        </sts>
        <cmds>
            <sends />
